( Root (span 1 89)
  ( Nucleus (span 1 73) (rel2par Topic-Shift)
    ( Nucleus (span 1 65) (rel2par Topic-Drift)
      ( Nucleus (span 1 30) (rel2par span)
        ( Nucleus (span 1 24) (rel2par span)
          ( Nucleus (span 1 3) (rel2par span)
            ( Nucleus (span 1 2) (rel2par Same-Unit)
              ( Nucleus (leaf 1) (rel2par span) (text _!The real estate slump_!) )
              ( Satellite (leaf 2) (rel2par elaboration-object-attribute-e) (text _!that's pushing down the price of New York office space and housing_!) )
            )
            ( Nucleus (leaf 3) (rel2par Same-Unit) (text _!is also affecting the city's retail real estate market.<P>_!) )
          )
          ( Satellite (span 4 24) (rel2par explanation-argumentative)
            ( Nucleus (span 4 9) (rel2par span)
              ( Nucleus (span 4 5) (rel2par span)
                ( Nucleus (leaf 4) (rel2par List) (text _!In Manhattan, once-desirable store sites sit vacant_!) )
                ( Nucleus (leaf 5) (rel2par List) (text _!and newly constructed space has been slow to fill._!) )
              )
              ( Satellite (span 6 9) (rel2par elaboration-additional)
                ( Satellite (leaf 6) (rel2par attribution) (text _!Retail real estate brokers say_!) )
                ( Nucleus (span 7 9) (rel2par span)
                  ( Nucleus (leaf 7) (rel2par Cause-Result) (text _!tenants are reluctant to sign leases_!) )
                  ( Nucleus (span 8 9) (rel2par Cause-Result)
                    ( Nucleus (leaf 8) (rel2par span) (text _!because of uncertainty about the local economy, turmoil in their own industries and a belief_!) )
                    ( Satellite (leaf 9) (rel2par elaboration-object-attribute-e) (text _!that rents have not yet hit bottom.<P>_!) )
                  )
                )
              )
            )
            ( Satellite (span 10 24) (rel2par elaboration-additional)
              ( Nucleus (span 10 20) (rel2par span)
                ( Nucleus (span 10 11) (rel2par span)
                  ( Nucleus (leaf 10) (rel2par span) (text _!"There is an unbelievable amount of space available,"_!) )
                  ( Satellite (leaf 11) (rel2par attribution) (text _!says Faith Consolo, senior vice president at Garrick-Aug Associates Store Leasing Inc._!) )
                )
                ( Satellite (span 12 20) (rel2par elaboration-additional)
                  ( Nucleus (leaf 12) (rel2par span) (text _!There are about 2,000 stores for rent, up from a more typical range of 1,200 to 1,500._!) )
                  ( Satellite (span 13 20) (rel2par elaboration-additional)
                    ( Nucleus (span 13 14) (rel2par span)
                      ( Nucleus (leaf 13) (rel2par span) (text _!"This further confuses retailers,"_!) )
                      ( Satellite (leaf 14) (rel2par attribution) (text _!she says._!) )
                    )
                    ( Satellite (span 15 20) (rel2par elaboration-additional)
                      ( Satellite (leaf 15) (rel2par attribution) (text _!"They wonder_!) )
                      ( Nucleus (span 16 20) (rel2par span)
                        ( Nucleus (span 16 17) (rel2par List)
                          ( Nucleus (leaf 16) (rel2par span) (text _!should they sign a lease_!) )
                          ( Satellite (leaf 17) (rel2par condition) (text _!if prices are still coming down?_!) )
                        )
                        ( Nucleus (span 18 19) (rel2par List)
                          ( Nucleus (leaf 18) (rel2par span) (text _!Is this the wrong time_!) )
                          ( Satellite (leaf 19) (rel2par elaboration-object-attribute-e) (text _!to open a store?_!) )
                        )
                        ( Nucleus (leaf 20) (rel2par List) (text _!Who is going to be in the space next door?"<P>_!) )
                      )
                    )
                  )
                )
              )
              ( Satellite (span 21 24) (rel2par elaboration-additional)
                ( Nucleus (span 21 22) (rel2par Same-Unit)
                  ( Nucleus (leaf 21) (rel2par span) (text _!In addition,_!) )
                  ( Satellite (leaf 22) (rel2par attribution-e) (text _!Ms. Consolo says,_!) )
                )
                ( Nucleus (span 23 24) (rel2par Same-Unit)
                  ( Nucleus (leaf 23) (rel2par span) (text _!tenants usually can negotiate to pay rents_!) )
                  ( Satellite (leaf 24) (rel2par elaboration-object-attribute-e) (text _!that are about one-quarter lower than landlords' initial asking price.<P>_!) )
                )
              )
            )
          )
        )
        ( Satellite (span 25 30) (rel2par elaboration-additional)
          ( Satellite (span 25 26) (rel2par antithesis)
            ( Nucleus (leaf 25) (rel2par span) (text _!A handful of hot retail locations, such as the 57th Street and Madison and Fifth Avenue areas, have been able to sustain what many see as astronomical rents._!) )
            ( Satellite (leaf 26) (rel2par elaboration-additional) (text _!And, in some neighborhoods, rents have merely hit a plateau._!) )
          )
          ( Nucleus (span 27 30) (rel2par span)
            ( Nucleus (span 27 28) (rel2par span)
              ( Nucleus (leaf 27) (rel2par span) (text _!But on average, Manhattan retail rents have dropped 10% to 15% in the past six months alone,_!) )
              ( Satellite (leaf 28) (rel2par attribution) (text _!experts say._!) )
            )
            ( Satellite (span 29 30) (rel2par elaboration-additional)
              ( Nucleus (leaf 29) (rel2par span) (text _!That follows a more subtle decline in the prior six months,_!) )
              ( Satellite (leaf 30) (rel2par circumstance) (text _!after Manhattan rents had run up rapidly since 1986.<P>_!) )
            )
          )
        )
      )
      ( Satellite (span 31 65) (rel2par elaboration-additional)
        ( Nucleus (span 31 59) (rel2par span)
          ( Nucleus (span 31 33) (rel2par span)
            ( Nucleus (span 31 32) (rel2par Same-Unit)
              ( Nucleus (leaf 31) (rel2par span) (text _!The same factors_!) )
              ( Satellite (leaf 32) (rel2par elaboration-object-attribute-e) (text _!limiting demand for office space_!) )
            )
            ( Nucleus (leaf 33) (rel2par Same-Unit) (text _!have affected retailing._!) )
          )
          ( Satellite (span 34 59) (rel2par explanation-argumentative)
            ( Nucleus (span 34 38) (rel2par span)
              ( Nucleus (span 34 37) (rel2par span)
                ( Satellite (leaf 34) (rel2par circumstance) (text _!"As businesses contract or depart,_!) )
                ( Nucleus (span 35 37) (rel2par span)
                  ( Nucleus (span 35 36) (rel2par Same-Unit)
                    ( Nucleus (leaf 35) (rel2par span) (text _!the number of employees_!) )
                    ( Satellite (leaf 36) (rel2par elaboration-object-attribute-e) (text _!who might use retail services_!) )
                  )
                  ( Nucleus (leaf 37) (rel2par Same-Unit) (text _!shrinks,"_!) )
                )
              )
              ( Satellite (leaf 38) (rel2par attribution) (text _!says Edward A. Friedman, senior vice president of Helmsley Spear Inc.<P>_!) )
            )
            ( Satellite (span 39 59) (rel2par elaboration-additional)
              ( Nucleus (span 39 50) (rel2par span)
                ( Nucleus (span 39 43) (rel2par span)
                  ( Satellite (leaf 39) (rel2par attribution) (text _!He says_!) )
                  ( Nucleus (span 40 43) (rel2par span)
                    ( Nucleus (span 40 42) (rel2par Same-Unit)
                      ( Nucleus (leaf 40) (rel2par span) (text _!financial problems_!) )
                      ( Satellite (span 41 42) (rel2par elaboration-object-attribute-e)
                        ( Nucleus (leaf 41) (rel2par span) (text _!plaguing electronics, fur and furniture companies_!) )
                        ( Satellite (leaf 42) (rel2par elaboration-additional-e) (text _!-- key categories in the local retail economy --_!) )
                      )
                    )
                    ( Nucleus (leaf 43) (rel2par Same-Unit) (text _!have further deflated the market._!) )
                  )
                )
                ( Satellite (span 44 50) (rel2par elaboration-additional)
                  ( Nucleus (span 44 45) (rel2par span)
                    ( Nucleus (leaf 44) (rel2par span) (text _!Hardest hit are what he calls "secondary" sites_!) )
                    ( Satellite (leaf 45) (rel2par elaboration-object-attribute-e) (text _!that primarily serve neighborhood residents._!) )
                  )
                  ( Satellite (span 46 50) (rel2par elaboration-additional)
                    ( Nucleus (span 46 48) (rel2par List)
                      ( Nucleus (span 46 47) (rel2par Same-Unit)
                        ( Nucleus (leaf 46) (rel2par span) (text _!In these locations,_!) )
                        ( Satellite (leaf 47) (rel2par attribution-e) (text _!Mr. Friedman says,_!) )
                      )
                      ( Nucleus (leaf 48) (rel2par Same-Unit) (text _!"Retailers are increasingly cautious about expanding_!) )
                    )
                    ( Nucleus (span 49 50) (rel2par List)
                      ( Nucleus (leaf 49) (rel2par Otherwise) (text _!and rents have remained steady_!) )
                      ( Nucleus (leaf 50) (rel2par Otherwise) (text _!or in some cases have declined."_!) )
                    )
                  )
                )
              )
              ( Satellite (span 51 59) (rel2par elaboration-additional)
                ( Nucleus (span 51 53) (rel2par span)
                  ( Nucleus (span 51 52) (rel2par Same-Unit)
                    ( Nucleus (leaf 51) (rel2par span) (text _!Weakness in the restaurant industry,_!) )
                    ( Satellite (leaf 52) (rel2par elaboration-additional-e) (text _!which is leaving retail space vacant,_!) )
                  )
                  ( Nucleus (leaf 53) (rel2par Same-Unit) (text _!exacerbates the problem for landlords.<P>_!) )
                )
                ( Satellite (span 54 59) (rel2par elaboration-additional)
                  ( Nucleus (span 54 57) (rel2par span)
                    ( Nucleus (leaf 54) (rel2par span) (text _!It is also no comfort to landlords and small New York retailers_!) )
                    ( Satellite (span 55 57) (rel2par circumstance)
                      ( Nucleus (span 55 56) (rel2par Same-Unit)
                        ( Nucleus (leaf 55) (rel2par span) (text _!when the future of larger department stores,_!) )
                        ( Satellite (leaf 56) (rel2par elaboration-additional-e) (text _!which anchor retail neighborhoods,_!) )
                      )
                      ( Nucleus (leaf 57) (rel2par Same-Unit) (text _!are in doubt._!) )
                    )
                  )
                  ( Satellite (span 58 59) (rel2par example)
                    ( Nucleus (leaf 58) (rel2par List) (text _!Hooker Corp., parent of Bonwit Teller and B. Altman's, is mired in bankruptcy proceedings_!) )
                    ( Nucleus (leaf 59) (rel2par List) (text _!and Bloomingdale's is for sale by its owner, Campeau Corp.<P>_!) )
                  )
                )
              )
            )
          )
        )
        ( Satellite (span 60 65) (rel2par comment)
          ( Satellite (span 60 61) (rel2par antithesis)
            ( Nucleus (leaf 60) (rel2par span) (text _!The trend toward lower rents may seem surprising_!) )
            ( Satellite (leaf 61) (rel2par circumstance) (text _!given that some communities in New York are bemoaning the loss of favorite local businesses to high rents._!) )
          )
          ( Nucleus (span 62 65) (rel2par span)
            ( Nucleus (leaf 62) (rel2par Same-Unit) (text _!But,_!) )
            ( Nucleus (span 63 65) (rel2par Same-Unit)
              ( Satellite (leaf 63) (rel2par concession-e) (text _!despite the recent softening,_!) )
              ( Nucleus (span 64 65) (rel2par span)
                ( Nucleus (leaf 64) (rel2par span) (text _!for many of these retailers there's still been too big a jump from the rental rates of the late 1970s,_!) )
                ( Satellite (leaf 65) (rel2par temporal-same-time) (text _!when their leases were signed.<P>_!) )
              )
            )
          )
        )
      )
    )
    ( Nucleus (span 66 73) (rel2par Topic-Drift)
      ( Nucleus (leaf 66) (rel2par span) (text _!Certainly, the recent drop in prices doesn't mean Manhattan comes cheap._!) )
      ( Satellite (span 67 73) (rel2par elaboration-set-member)
        ( Nucleus (span 67 71) (rel2par Contrast)
          ( Nucleus (leaf 67) (rel2par span) (text _!New York retail rents still run well above the going rate in other U.S. cities._!) )
          ( Satellite (span 68 71) (rel2par explanation-argumentative)
            ( Nucleus (span 68 69) (rel2par span)
              ( Nucleus (leaf 68) (rel2par List) (text _!Madison and Fifth Avenues and East 57th Street can command rents of up to $500 a square foot,_!) )
              ( Nucleus (leaf 69) (rel2par List) (text _!and $250 is not uncommon._!) )
            )
            ( Satellite (span 70 71) (rel2par elaboration-additional)
              ( Nucleus (leaf 70) (rel2par List) (text _!The thriving 34th Street area offers rents of about $100 a square foot,_!) )
              ( Nucleus (leaf 71) (rel2par List) (text _!as do up-and-coming locations along lower Fifth Avenue._!) )
            )
          )
        )
        ( Nucleus (span 72 73) (rel2par Contrast)
          ( Nucleus (leaf 72) (rel2par List) (text _!By contrast, rentals in the best retail locations in Boston, San Francisco and Chicago rarely top $100 a square foot._!) )
          ( Nucleus (leaf 73) (rel2par List) (text _!And rents on Beverly Hills' Rodeo Drive generally don't exceed about $125 a square foot.<P>_!) )
        )
      )
    )
  )
