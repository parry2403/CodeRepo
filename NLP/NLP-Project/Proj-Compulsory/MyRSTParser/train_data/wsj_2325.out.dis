( Root (span 1 79)
  ( Nucleus (leaf 1) (rel2par span) (text _!The oil industry's middling profits could persist through the rest of the year.<P>_!) )
  ( Satellite (span 2 79) (rel2par summary-n)
    ( Nucleus (span 2 60) (rel2par span)
      ( Nucleus (span 2 48) (rel2par span)
        ( Nucleus (span 2 33) (rel2par span)
          ( Nucleus (span 2 9) (rel2par span)
            ( Nucleus (span 2 4) (rel2par span)
              ( Nucleus (span 2 3) (rel2par span)
                ( Nucleus (leaf 2) (rel2par span) (text _!Major oil companies in the next few days are expected to report much less robust earnings_!) )
                ( Satellite (leaf 3) (rel2par comparison) (text _!than they did for the third quarter a year ago,_!) )
              )
              ( Satellite (leaf 4) (rel2par elaboration-object-attribute-e) (text _!largely reflecting deteriorating chemical prices and gasoline profitability._!) )
            )
            ( Satellite (span 5 9) (rel2par elaboration-additional)
              ( Nucleus (span 5 7) (rel2par span)
                ( Nucleus (span 5 6) (rel2par span)
                  ( Nucleus (leaf 5) (rel2par Contrast) (text _!The gasoline picture may improve this quarter,_!) )
                  ( Nucleus (leaf 6) (rel2par Contrast) (text _!but chemicals are likely to remain weak,_!) )
                )
                ( Satellite (leaf 7) (rel2par attribution) (text _!industry executives and analysts say,_!) )
              )
              ( Satellite (span 8 9) (rel2par consequence-s)
                ( Nucleus (leaf 8) (rel2par span) (text _!reducing chances_!) )
                ( Satellite (leaf 9) (rel2par elaboration-object-attribute-e) (text _!that profits could equal their year-earlier performance.<P>_!) )
              )
            )
          )
          ( Satellite (span 10 33) (rel2par elaboration-additional)
            ( Nucleus (span 10 25) (rel2par span)
              ( Nucleus (span 10 20) (rel2par span)
                ( Nucleus (span 10 11) (rel2par span)
                  ( Nucleus (leaf 10) (rel2par span) (text _!The industry is "seeing a softening somewhat in volume and certainly in price in petrochemicals,"_!) )
                  ( Satellite (leaf 11) (rel2par attribution) (text _!Glenn Cox, president of Phillips Petroleum Co., said in an interview._!) )
                )
                ( Satellite (span 12 20) (rel2par elaboration-additional)
                  ( Nucleus (span 12 13) (rel2par span)
                    ( Nucleus (leaf 12) (rel2par span) (text _!"That change will obviously impact third and fourth quarter earnings" for the industry in general,_!) )
                    ( Satellite (leaf 13) (rel2par attribution) (text _!he added._!) )
                  )
                  ( Satellite (span 14 20) (rel2par example)
                    ( Nucleus (span 14 17) (rel2par List)
                      ( Satellite (leaf 14) (rel2par antithesis) (text _!He didn't forecast Phillips's results.<P>_!) )
                      ( Nucleus (span 15 17) (rel2par span)
                        ( Satellite (leaf 15) (rel2par attribution) (text _!But securities analysts say_!) )
                        ( Nucleus (span 16 17) (rel2par span)
                          ( Nucleus (leaf 16) (rel2par span) (text _!Phillips will be among the companies hard-hit by weak chemical prices_!) )
                          ( Satellite (leaf 17) (rel2par consequence-s) (text _!and will probably post a drop in third-quarter earnings._!) )
                        )
                      )
                    )
                    ( Nucleus (span 18 20) (rel2par List)
                      ( Nucleus (span 18 19) (rel2par Same-Unit)
                        ( Nucleus (leaf 18) (rel2par span) (text _!So, too,_!) )
                        ( Satellite (leaf 19) (rel2par attribution-e) (text _!many analysts predict,_!) )
                      )
                      ( Nucleus (leaf 20) (rel2par Same-Unit) (text _!will Exxon Corp., Chevron Corp. and Amoco Corp.<P>_!) )
                    )
                  )
                )
              )
              ( Satellite (span 21 25) (rel2par elaboration-additional)
                ( Nucleus (span 21 22) (rel2par span)
                  ( Nucleus (leaf 21) (rel2par span) (text _!Typical is what happened to the price of ethylene, a major commodity chemical_!) )
                  ( Satellite (leaf 22) (rel2par elaboration-object-attribute-e) (text _!produced in vast amounts by many oil companies._!) )
                )
                ( Satellite (span 23 25) (rel2par elaboration-general-specific)
                  ( Nucleus (leaf 23) (rel2par span) (text _!It has plunged 13% since July to around 26 cents a pound._!) )
                  ( Satellite (span 24 25) (rel2par comparison)
                    ( Nucleus (leaf 24) (rel2par span) (text _!A year ago ethylene sold for 33 cents,_!) )
                    ( Satellite (leaf 25) (rel2par elaboration-additional) (text _!peaking at about 34 cents last December.<P>_!) )
                  )
                )
              )
            )
            ( Satellite (span 26 33) (rel2par elaboration-additional)
              ( Nucleus (leaf 26) (rel2par span) (text _!A big reason for the chemical price retreat is overexpansion._!) )
              ( Satellite (span 27 33) (rel2par elaboration-additional)
                ( Satellite (span 27 30) (rel2par result)
                  ( Nucleus (span 27 29) (rel2par span)
                    ( Satellite (leaf 27) (rel2par circumstance) (text _!Beginning in mid-1987,_!) )
                    ( Nucleus (span 28 29) (rel2par span)
                      ( Nucleus (leaf 28) (rel2par span) (text _!prices began accelerating_!) )
                      ( Satellite (leaf 29) (rel2par explanation-argumentative) (text _!as a growing U.S. economy and the weak dollar spurred demand._!) )
                    )
                  )
                  ( Satellite (leaf 30) (rel2par consequence-s) (text _!Companies added capacity furiously._!) )
                )
                ( Nucleus (span 31 33) (rel2par span)
                  ( Satellite (leaf 31) (rel2par circumstance) (text _!Now, greatly increased supplies are on the market,_!) )
                  ( Nucleus (span 32 33) (rel2par span)
                    ( Nucleus (leaf 32) (rel2par List) (text _!while the dollar is stronger,_!) )
                    ( Nucleus (leaf 33) (rel2par List) (text _!and domestic economic growth is slower.<P>_!) )
                  )
                )
              )
            )
          )
        )
        ( Satellite (span 34 48) (rel2par elaboration-additional)
          ( Nucleus (span 34 43) (rel2par span)
            ( Nucleus (span 34 37) (rel2par span)
              ( Nucleus (leaf 34) (rel2par span) (text _!Third-quarter profits from gasoline were weaker._!) )
              ( Satellite (span 35 37) (rel2par elaboration-additional)
                ( Nucleus (span 35 36) (rel2par span)
                  ( Satellite (leaf 35) (rel2par comparison) (text _!"Refining margins were so good in the third quarter of last year_!) )
                  ( Nucleus (leaf 36) (rel2par span) (text _!and generally not very good this year,"_!) )
                )
                ( Satellite (leaf 37) (rel2par attribution) (text _!said William Randol, a securities analyst at First Boston Corp.<P>_!) )
              )
            )
            ( Satellite (span 38 43) (rel2par explanation-argumentative)
              ( Nucleus (span 38 41) (rel2par span)
                ( Satellite (span 38 40) (rel2par consequence-n)
                  ( Nucleus (leaf 38) (rel2par span) (text _!Oil company refineries ran flat out_!) )
                  ( Satellite (span 39 40) (rel2par purpose)
                    ( Nucleus (leaf 39) (rel2par span) (text _!to prepare for a robust holiday driving season in July and August_!) )
                    ( Satellite (leaf 40) (rel2par elaboration-object-attribute-e) (text _!that didn't materialize._!) )
                  )
                )
                ( Nucleus (leaf 41) (rel2par span) (text _!The excess supply pushed gasoline prices down in that period._!) )
              )
              ( Satellite (span 42 43) (rel2par elaboration-additional)
                ( Nucleus (leaf 42) (rel2par span) (text _!In addition, crude oil prices were up some from a year earlier,_!) )
                ( Satellite (leaf 43) (rel2par consequence-s) (text _!further pressuring profitability.<P>_!) )
              )
            )
          )
          ( Satellite (span 44 48) (rel2par antithesis)
            ( Nucleus (span 44 45) (rel2par span)
              ( Satellite (leaf 44) (rel2par attribution) (text _!Refiners say_!) )
              ( Nucleus (leaf 45) (rel2par span) (text _!margins picked up in September,_!) )
            )
            ( Satellite (span 46 48) (rel2par consequence-s)
              ( Nucleus (span 46 47) (rel2par span)
                ( Satellite (leaf 46) (rel2par attribution) (text _!and many industry officials believe_!) )
                ( Nucleus (leaf 47) (rel2par span) (text _!gasoline profits will rebound this quarter, though still not to the level of 1988's fourth quarter._!) )
              )
              ( Satellite (leaf 48) (rel2par elaboration-additional) (text _!During the 1988 second half, many companies posted record gasoline and chemical profits.<P>_!) )
            )
          )
        )
      )
      ( Satellite (span 49 60) (rel2par elaboration-additional)
        ( Nucleus (leaf 49) (rel2par span) (text _!Crude oil production may turn out to be the most surprising element of companies' earnings this year._!) )
        ( Satellite (span 50 60) (rel2par elaboration-additional)
          ( Satellite (span 50 57) (rel2par interpretation-n)
            ( Nucleus (span 50 52) (rel2par List)
              ( Nucleus (span 50 51) (rel2par Same-Unit)
                ( Nucleus (leaf 50) (rel2par span) (text _!Prices_!) )
                ( Satellite (leaf 51) (rel2par elaboration-additional-e) (text _!-- averaging roughly $2 a barrel higher in the third quarter than a year earlier --_!) )
              )
              ( Nucleus (leaf 52) (rel2par Same-Unit) (text _!have stayed well above most companies' expectations._!) )
            )
            ( Nucleus (span 53 57) (rel2par List)
              ( Nucleus (span 53 55) (rel2par span)
                ( Nucleus (span 53 54) (rel2par List)
                  ( Nucleus (leaf 53) (rel2par span) (text _!Demand has been much stronger_!) )
                  ( Satellite (leaf 54) (rel2par comparison) (text _!than anticipated,_!) )
                )
                ( Nucleus (leaf 55) (rel2par List) (text _!and it typically accelerates in the fourth quarter._!) )
              )
              ( Satellite (span 56 57) (rel2par consequence-s)
                ( Nucleus (leaf 56) (rel2par span) (text _!"We could see higher oil prices this year,"_!) )
                ( Satellite (leaf 57) (rel2par attribution) (text _!said Bryan Jacoboski, an analyst at PaineWebber Inc.<P>_!) )
              )
            )
          )
          ( Nucleus (span 58 60) (rel2par span)
            ( Nucleus (leaf 58) (rel2par span) (text _!That will translate into sharply higher production profits,_!) )
            ( Satellite (span 59 60) (rel2par comparison)
              ( Nucleus (leaf 59) (rel2par span) (text _!particularly compared with last year_!) )
              ( Satellite (leaf 60) (rel2par circumstance) (text _!when oil prices steadily fell to below $13 a barrel in the fourth quarter.<P>_!) )
            )
          )
        )
