            ( Satellite (span 67 69) (rel2par antithesis)
              ( Nucleus (leaf 67) (rel2par span) (text _!Last summer was notable for a heat wave and drought_!) )
              ( Satellite (span 68 69) (rel2par elaboration-object-attribute-e)
                ( Nucleus (leaf 68) (rel2par span) (text _!that caused utilities to burn more natural gas_!) )
                ( Satellite (leaf 69) (rel2par purpose) (text _!to feed increased electrical demand from air conditioning use._!) )
