      ( Satellite (span 71 79) (rel2par elaboration-additional)
        ( Nucleus (span 71 72) (rel2par span)
          ( Nucleus (leaf 71) (rel2par span) (text _!"We've been very disappointed in the performance of natural gas prices,"_!) )
          ( Satellite (leaf 72) (rel2par attribution) (text _!said Mr. Cox, Phillips's president._!) )
        )
        ( Satellite (span 73 79) (rel2par elaboration-additional)
          ( Nucleus (span 73 74) (rel2par span)
            ( Nucleus (leaf 73) (rel2par span) (text _!"The lagging gas price is not going to assist fourth quarter performance_!) )
            ( Satellite (leaf 74) (rel2par comparison) (text _!as many had expected."<P>_!) )
