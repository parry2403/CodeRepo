( Root (span 1 75)
  ( Nucleus (span 1 58) (rel2par span)
    ( Nucleus (span 1 38) (rel2par span)
      ( Nucleus (span 1 13) (rel2par span)
        ( Nucleus (span 1 4) (rel2par span)
          ( Nucleus (span 1 2) (rel2par Same-Unit)
            ( Nucleus (leaf 1) (rel2par span) (text _!Eastman Kodak Co.,_!) )
            ( Satellite (leaf 2) (rel2par elaboration-object-attribute) (text _!seeking to position itself in the potentially huge high-definition television market,_!) )
          )
          ( Nucleus (span 3 4) (rel2par Same-Unit)
            ( Nucleus (leaf 3) (rel2par span) (text _!unveiled a converter_!) )
            ( Satellite (leaf 4) (rel2par elaboration-object-attribute-e) (text _!that can transform conventional motion-picture film into high-definition video.<P>_!) )
          )
        )
        ( Satellite (span 5 13) (rel2par elaboration-additional)
          ( Nucleus (span 5 9) (rel2par span)
            ( Nucleus (leaf 5) (rel2par span) (text _!The move also helps the Rochester, N.Y., photographic giant ensure_!) )
            ( Satellite (span 6 9) (rel2par purpose)
              ( Nucleus (span 6 8) (rel2par Same-Unit)
                ( Nucleus (leaf 6) (rel2par span) (text _!that its motion-picture film business_!) )
                ( Satellite (span 7 8) (rel2par elaboration-object-attribute)
                  ( Nucleus (leaf 7) (rel2par List) (text _!-- for which it holds a virtual monopoly,_!) )
                  ( Nucleus (leaf 8) (rel2par List) (text _!supplying every Hollywood movie company --_!) )
                )
              )
              ( Nucleus (leaf 9) (rel2par Same-Unit) (text _!isn't made obsolete by the upstart HDTV business.<P>_!) )
            )
          )
          ( Satellite (span 10 13) (rel2par evaluation-s)
            ( Satellite (leaf 10) (rel2par concession) (text _!While the prototype converter is costly,_!) )
            ( Nucleus (span 11 13) (rel2par span)
              ( Nucleus (leaf 11) (rel2par span) (text _!it's being lauded by the infant HDTV industry as a way_!) )
              ( Satellite (span 12 13) (rel2par elaboration-object-attribute-e)
                ( Nucleus (leaf 12) (rel2par span) (text _!of increasing the number of high-quality shows_!) )
                ( Satellite (leaf 13) (rel2par elaboration-object-attribute-e) (text _!that can be seen on the new medium.<P>_!) )
              )
            )
          )
        )
      )
      ( Satellite (span 14 38) (rel2par background)
        ( Nucleus (span 14 15) (rel2par span)
          ( Nucleus (leaf 14) (rel2par span) (text _!"The industry has been waiting with bated breath for the machines to come along,"_!) )
          ( Satellite (leaf 15) (rel2par attribution) (text _!says David Niles, president of Eleven Twenty Five Productions Inc., a New York pioneer in high-definition programming._!) )
        )
        ( Satellite (span 16 38) (rel2par elaboration-additional)
          ( Satellite (leaf 16) (rel2par attribution) (text _!He notes_!) )
          ( Nucleus (span 17 38) (rel2par span)
            ( Nucleus (leaf 17) (rel2par span) (text _!that industry executives have until now worried_!) )
            ( Satellite (span 18 38) (rel2par cause)
              ( Nucleus (span 18 23) (rel2par span)
                ( Nucleus (span 18 19) (rel2par span)
                  ( Nucleus (leaf 18) (rel2par span) (text _!that they would face a severe shortage of programs_!) )
                  ( Satellite (leaf 19) (rel2par contingency) (text _!once consumers begin replacing their TV sets with HDTVs.<P>_!) )
                )
                ( Satellite (span 20 23) (rel2par reason)
                  ( Nucleus (span 20 22) (rel2par span)
                    ( Nucleus (leaf 20) (rel2par List) (text _!Japanese electronic giants, such as Sony Corp. and Hitachi Ltd., have focused almost entirely on HDTV hardware,_!) )
                    ( Nucleus (leaf 21) (rel2par List) (text _!and virtually ignored software or programs shot in high-definition._!) )
                    ( Nucleus (leaf 22) (rel2par List) (text _!And only a handful of small U.S. companies are engaged in high-definition software development._!) )
                  )
                  ( Satellite (leaf 23) (rel2par restatement) (text _!It's estimated that just about 250 hours of HD programming is currently available for airing.<P>_!) )
                )
              )
              ( Satellite (span 24 38) (rel2par problem-solution-s)
                ( Nucleus (span 24 36) (rel2par span)
                  ( Nucleus (span 24 34) (rel2par span)
                    ( Satellite (leaf 24) (rel2par attribution) (text _!Kodak says_!) )
                    ( Nucleus (span 25 34) (rel2par span)
                      ( Nucleus (leaf 25) (rel2par span) (text _!its new CCD HDTV converter will help alleviate the problem_!) )
                      ( Satellite (span 26 34) (rel2par means)
                        ( Nucleus (span 26 31) (rel2par Otherwise)
                          ( Nucleus (span 26 27) (rel2par Same-Unit)
                            ( Nucleus (leaf 26) (rel2par span) (text _!by allowing programmers and broadcasters to convert movies and television programs_!) )
                            ( Satellite (leaf 27) (rel2par elaboration-object-attribute-e) (text _!shot in 35mm motion-picture film_!) )
                          )
                          ( Nucleus (span 28 31) (rel2par Same-Unit)
                            ( Nucleus (leaf 28) (rel2par span) (text _!into high-definition video._!) )
                            ( Satellite (span 29 31) (rel2par result)
                              ( Nucleus (leaf 29) (rel2par List) (text _!Consumers will be able to switch on their HDTV sets_!) )
                              ( Nucleus (span 30 31) (rel2par List)
                                ( Nucleus (leaf 30) (rel2par span) (text _!and get all the viewing benefits_!) )
                                ( Satellite (leaf 31) (rel2par elaboration-object-attribute-e) (text _!the high-tech medium offers._!) )
                              )
                            )
                          )
                        )
                        ( Nucleus (span 32 34) (rel2par Otherwise)
                          ( Nucleus (leaf 32) (rel2par span) (text _!Otherwise, they'd be watching programs_!) )
                          ( Satellite (span 33 34) (rel2par elaboration-object-attribute-e)
                            ( Nucleus (leaf 33) (rel2par span) (text _!that are no different in quality_!) )
                            ( Satellite (leaf 34) (rel2par comparison) (text _!from what they currently view on color TVs.<P>_!) )
                          )
                        )
                      )
                    )
                  )
                  ( Satellite (span 35 36) (rel2par comment)
                    ( Nucleus (leaf 35) (rel2par span) (text _!It would be like "watching a black and white movie on a color TV set,"_!) )
                    ( Satellite (leaf 36) (rel2par attribution) (text _!says Malcolm G. Saull, chairman of the film and video department at the Rochester Institute of Technology.<P>_!) )
                  )
                )
                ( Satellite (span 37 38) (rel2par evaluation-s)
                  ( Nucleus (leaf 37) (rel2par span) (text _!The new converters are "a critical link between film and the television domain,"_!) )
                  ( Satellite (leaf 38) (rel2par attribution) (text _!says Joerg D. Agin, vice president and general manager of Kodak's Motion Picture and Audiovisual Products division.<P>_!) )
                )
              )
            )
          )
        )
      )
    )
    ( Satellite (span 39 58) (rel2par elaboration-additional)
      ( Nucleus (span 39 49) (rel2par span)
        ( Nucleus (span 39 40) (rel2par span)
          ( Nucleus (leaf 39) (rel2par span) (text _!Kodak won't disclose the cost or when its converter will be on the market,_!) )
          ( Satellite (leaf 40) (rel2par antithesis) (text _!but it's estimated the machine may be available within two years._!) )
        )
        ( Satellite (span 41 49) (rel2par elaboration-additional)
          ( Nucleus (span 41 43) (rel2par List)
            ( Nucleus (span 41 42) (rel2par Same-Unit)
              ( Nucleus (leaf 41) (rel2par span) (text _!A similar machine already on the market,_!) )
              ( Satellite (leaf 42) (rel2par elaboration-object-attribute-e) (text _!made by Rank Sintel Ltd., a unit of Rank Organisation,_!) )
            )
            ( Nucleus (leaf 43) (rel2par Same-Unit) (text _!costs about $500,000.<P>_!) )
          )
          ( Nucleus (span 44 49) (rel2par List)
            ( Nucleus (span 44 45) (rel2par span)
              ( Nucleus (leaf 44) (rel2par span) (text _!And the potential market is tremendous,_!) )
              ( Satellite (leaf 45) (rel2par attribution) (text _!industry experts say._!) )
            )
            ( Satellite (span 46 49) (rel2par example)
              ( Nucleus (span 46 48) (rel2par span)
                ( Satellite (leaf 46) (rel2par condition) (text _!If HDTV takes off in the U.S.,_!) )
                ( Nucleus (span 47 48) (rel2par span)
                  ( Nucleus (leaf 47) (rel2par span) (text _!there will be demand for some 4,000 to 5,000 HDTV converters,_!) )
                  ( Satellite (leaf 48) (rel2par elaboration-object-attribute-e) (text _!known in the industry as telecines._!) )
                )
              )
              ( Satellite (leaf 49) (rel2par elaboration-additional) (text _!Demand will come first from programming production companies and then from television stations.<P>_!) )
            )
          )
        )
      )
      ( Satellite (span 50 58) (rel2par evaluation-s)
        ( Nucleus (span 50 54) (rel2par span)
          ( Nucleus (span 50 51) (rel2par List)
            ( Nucleus (leaf 50) (rel2par span) (text _!"The converter is head and shoulders above anything else I've seen,"_!) )
            ( Satellite (leaf 51) (rel2par attribution) (text _!says Richard J. Stumpf, vice president-engineering and development at MCA Inc. 's Universal City Studios._!) )
          )
          ( Nucleus (span 52 54) (rel2par List)
            ( Satellite (leaf 52) (rel2par attribution) (text _!And Mr. Niles, the program producer, contends_!) )
            ( Nucleus (span 53 54) (rel2par span)
              ( Nucleus (leaf 53) (rel2par List) (text _!that Kodak's move is "a sound marketing decision._!) )
              ( Nucleus (leaf 54) (rel2par List) (text _!They can't afford to stay out of HDTV."<P>_!) )
            )
          )
        )
        ( Satellite (span 55 58) (rel2par elaboration-additional)
          ( Nucleus (leaf 55) (rel2par span) (text _!Indeed, the stakes are high._!) )
          ( Satellite (span 56 58) (rel2par elaboration-additional)
            ( Satellite (leaf 56) (rel2par attribution) (text _!The U.S. electronics industry estimates_!) )
            ( Nucleus (span 57 58) (rel2par span)
              ( Nucleus (leaf 57) (rel2par span) (text _!that the HDTV market will total about $150 billion over the next two decades, with an additional $400 billion_!) )
              ( Satellite (leaf 58) (rel2par elaboration-object-attribute-e) (text _!expected to go for related products.<P>_!) )
            )
          )
        )
      )
    )
  )
  ( Satellite (span 59 75) (rel2par summary-s)
    ( Nucleus (span 59 71) (rel2par span)
      ( Nucleus (span 59 63) (rel2par span)
        ( Nucleus (span 59 61) (rel2par List)
          ( Nucleus (span 59 60) (rel2par span)
            ( Nucleus (leaf 59) (rel2par span) (text _!HDTVs break down images into more than 1,100 lines,_!) )
            ( Satellite (leaf 60) (rel2par comparison) (text _!compared with 525 for today's televisions,_!) )
          )
          ( Satellite (leaf 61) (rel2par result) (text _!providing considerably sharper detail._!) )
        )
        ( Nucleus (span 62 63) (rel2par List)
          ( Nucleus (leaf 62) (rel2par span) (text _!And the sets are wider,_!) )
          ( Satellite (leaf 63) (rel2par elaboration-object-attribute) (text _!resembling the dimensions of a movie screen.<P>_!) )
        )
      )
      ( Satellite (span 64 71) (rel2par antithesis)
        ( Nucleus (span 64 65) (rel2par span)
          ( Nucleus (leaf 64) (rel2par List) (text _!But the financial rewards aren't expected soon,_!) )
          ( Nucleus (leaf 65) (rel2par List) (text _!nor are they guaranteed._!) )
        )
        ( Satellite (span 66 71) (rel2par elaboration-additional)
          ( Nucleus (span 66 68) (rel2par span)
            ( Satellite (leaf 66) (rel2par attribution) (text _!Experts estimate_!) )
            ( Nucleus (span 67 68) (rel2par span)
              ( Nucleus (leaf 67) (rel2par List) (text _!the first sets of HDTVs won't be available for another five to 10 years,_!) )
              ( Nucleus (leaf 68) (rel2par List) (text _!and will probably retail for more than $3,000 each in today's dollars._!) )
            )
          )
          ( Satellite (span 69 71) (rel2par comment)
            ( Satellite (leaf 69) (rel2par attribution) (text _!Some critics say_!) )
            ( Nucleus (span 70 71) (rel2par span)
              ( Nucleus (leaf 70) (rel2par span) (text _!they won't be quickly embraced by consumers_!) )
              ( Satellite (leaf 71) (rel2par reason) (text _!because of the high price.<P>_!) )
            )
          )
        )
      )
    )
    ( Satellite (span 72 75) (rel2par comment)
      ( Nucleus (leaf 72) (rel2par span) (text _!Nevertheless, Kodak couldn't risk letting HDTV turn its motion-picture film business into a dinosaur._!) )
      ( Satellite (span 73 75) (rel2par elaboration-additional)
        ( Nucleus (leaf 73) (rel2par span) (text _!"Kodak understands_!) )
        ( Satellite (span 74 75) (rel2par evaluation-s)
          ( Nucleus (leaf 74) (rel2par span) (text _!HDTV is where everybody is going,"_!) )
          ( Satellite (leaf 75) (rel2par attribution) (text _!says RIT's Mr. Spaull._!) )
        )
      )
    )
  )
)
