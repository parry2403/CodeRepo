( Root (span 1 57)
  ( Nucleus (span 1 52) (rel2par span)
    ( Nucleus (span 1 21) (rel2par span)
      ( Nucleus (span 1 5) (rel2par span)
        ( Satellite (leaf 1) (rel2par attribution) (text _!Interprovincial Pipe Line Co. said_!) )
        ( Nucleus (span 2 5) (rel2par span)
          ( Nucleus (span 2 4) (rel2par span)
            ( Nucleus (span 2 3) (rel2par Same-Unit)
              ( Nucleus (leaf 2) (rel2par span) (text _!it will delay a proposed two-step, 830 million Canadian-dollar_!) )
              ( Satellite (leaf 3) (rel2par restatement-e) (text _!(US$705.6 million)_!) )
            )
            ( Nucleus (leaf 4) (rel2par Same-Unit) (text _!expansion of its system_!) )
          )
          ( Satellite (leaf 5) (rel2par reason) (text _!because Canada's output of crude oil is shrinking.<P>_!) )
        )
      )
      ( Satellite (span 6 21) (rel2par explanation-argumentative)
        ( Nucleus (span 6 9) (rel2par span)
          ( Nucleus (span 6 8) (rel2par span)
            ( Satellite (leaf 6) (rel2par attribution) (text _!Interprovincial, Canada's biggest oil pipeline operator and a major transporter of crude to the U.S., said_!) )
            ( Nucleus (span 7 8) (rel2par span)
              ( Satellite (leaf 7) (rel2par attribution) (text _!revised industry forecasts indicate_!) )
              ( Nucleus (leaf 8) (rel2par span) (text _!that Canadian oil output will total about 1.64 million barrels a day by 1991, 8% lower than a previous estimate._!) )
            )
          )
          ( Satellite (leaf 9) (rel2par elaboration-additional) (text _!Canadian crude production averaged about 1.69 million barrels a day during 1989's first half, about 1% below the 1988 level.<P>_!) )
        )
        ( Satellite (span 10 21) (rel2par elaboration-additional)
          ( Nucleus (span 10 15) (rel2par span)
            ( Nucleus (span 10 14) (rel2par span)
              ( Nucleus (span 10 13) (rel2par span)
                ( Nucleus (span 10 12) (rel2par List)
                  ( Nucleus (span 10 11) (rel2par Same-Unit)
                    ( Nucleus (leaf 10) (rel2par span) (text _!"The capability of existing fields_!) )
                    ( Satellite (leaf 11) (rel2par elaboration-object-attribute-e) (text _!to deliver oil_!) )
                  )
                  ( Nucleus (leaf 12) (rel2par Same-Unit) (text _!is dropping,"_!) )
                )
                ( Nucleus (leaf 13) (rel2par List) (text _!and oil exploration activity is also down dramatically,_!) )
              )
              ( Satellite (leaf 14) (rel2par circumstance) (text _!as many producers shift their emphasis to natural gas,_!) )
            )
            ( Satellite (leaf 15) (rel2par attribution) (text _!said Ronald Watkins, vice president for government and industry relations with Interprovincial's parent, Interhome Energy Inc._!) )
          )
          ( Satellite (span 16 21) (rel2par elaboration-additional)
            ( Nucleus (span 16 19) (rel2par span)
              ( Satellite (leaf 16) (rel2par attribution) (text _!Mr. Watkins said_!) )
              ( Nucleus (span 17 19) (rel2par span)
                ( Nucleus (span 17 18) (rel2par Cause-Result)
                  ( Nucleus (leaf 17) (rel2par List) (text _!volume on Interprovincial's system is down about 2% since January_!) )
                  ( Nucleus (leaf 18) (rel2par List) (text _!and is expected to fall further,_!) )
                )
                ( Nucleus (leaf 19) (rel2par Cause-Result) (text _!making expansion unnecessary until perhaps the mid-1990s.<P>_!) )
              )
            )
            ( Satellite (span 20 21) (rel2par elaboration-additional)
              ( Nucleus (leaf 20) (rel2par span) (text _!"There has been a swing of the pendulum back to the gas side,"_!) )
              ( Satellite (leaf 21) (rel2par attribution) (text _!he said.<P>_!) )
            )
          )
        )
      )
    )
    ( Satellite (span 22 52) (rel2par evaluation-s)
      ( Nucleus (span 22 28) (rel2par span)
        ( Nucleus (span 22 24) (rel2par List)
          ( Satellite (leaf 22) (rel2par attribution) (text _!Many of Canada's oil and gas producers say_!) )
          ( Nucleus (span 23 24) (rel2par span)
            ( Nucleus (leaf 23) (rel2par Comparison) (text _!the outlook for natural gas is better_!) )
            ( Nucleus (leaf 24) (rel2par Comparison) (text _!than it is for oil,_!) )
          )
        )
        ( Nucleus (span 25 28) (rel2par List)
          ( Nucleus (leaf 25) (rel2par span) (text _!and have shifted their exploration and development budgets accordingly._!) )
          ( Satellite (span 26 28) (rel2par explanation-argumentative)
            ( Nucleus (span 26 27) (rel2par span)
              ( Nucleus (leaf 26) (rel2par List) (text _!The number of active drilling rigs in Canada is down 30% from a year ago,_!) )
              ( Nucleus (leaf 27) (rel2par List) (text _!and the number of completed oil wells is "down more than that, due to the increasing focus on gas exploration,"_!) )
            )
            ( Satellite (leaf 28) (rel2par attribution) (text _!said Robert Feick, manager of crude oil with Calgary's Independent Petroleum Association of Canada, an industry group.<P>_!) )
          )
        )
      )
      ( Satellite (span 29 52) (rel2par interpretation-s)
        ( Nucleus (span 29 33) (rel2par span)
          ( Nucleus (span 29 30) (rel2par span)
            ( Satellite (leaf 29) (rel2par attribution) (text _!Mr. Watkins said_!) )
            ( Nucleus (leaf 30) (rel2par span) (text _!the main reason for the production decline is shrinking output of light crude from mature, conventional fields in western Canada._!) )
          )
          ( Satellite (span 31 33) (rel2par elaboration-additional)
            ( Nucleus (span 31 32) (rel2par List)
              ( Nucleus (leaf 31) (rel2par span) (text _!Interprovincial transports about 75% of all crude_!) )
              ( Satellite (leaf 32) (rel2par elaboration-object-attribute-e) (text _!produced in western Canada,_!) )
            )
            ( Nucleus (leaf 33) (rel2par List) (text _!and almost 60% of Interprovincial's total volume consists of light crude.<P>_!) )
          )
        )
        ( Satellite (span 34 52) (rel2par circumstance)
          ( Nucleus (span 34 38) (rel2par span)
            ( Nucleus (span 34 35) (rel2par Same-Unit)
              ( Nucleus (leaf 34) (rel2par span) (text _!Nearly all of the crude oil_!) )
              ( Satellite (leaf 35) (rel2par elaboration-object-attribute-e) (text _!that Canada exports to the U.S._!) )
            )
            ( Nucleus (span 36 38) (rel2par Same-Unit)
              ( Nucleus (leaf 36) (rel2par span) (text _!is transported on Interprovincial's system,_!) )
              ( Satellite (span 37 38) (rel2par elaboration-part-whole-e)
                ( Nucleus (leaf 37) (rel2par span) (text _!whose main line runs from Edmonton to major U.S. and Canadian cities in the Great Lakes region,_!) )
                ( Satellite (leaf 38) (rel2par elaboration-part-whole-e) (text _!including Chicago, Buffalo, Toronto and Montreal.<P>_!) )
              )
            )
          )
          ( Satellite (span 39 52) (rel2par elaboration-additional)
            ( Nucleus (span 39 41) (rel2par span)
              ( Nucleus (span 39 40) (rel2par span)
                ( Nucleus (leaf 39) (rel2par span) (text _!Canada's current oil exports to the U.S. total about 600,000 barrels a day, or about 9.1% of net U.S. crude imports,_!) )
                ( Satellite (leaf 40) (rel2par attribution) (text _!said John Lichtblau, president of the New York-based Petroleum Industry Research Foundation._!) )
              )
              ( Satellite (leaf 41) (rel2par interpretation-s) (text _!That ranks Canada as the fourth-largest source of imported crude, behind Saudi Arabia, Nigeria and Mexico.<P>_!) )
            )
            ( Satellite (span 42 52) (rel2par elaboration-additional)
              ( Nucleus (span 42 45) (rel2par span)
                ( Satellite (leaf 42) (rel2par attribution) (text _!Mr. Lichtblau said_!) )
                ( Nucleus (span 43 45) (rel2par span)
                  ( Nucleus (span 43 44) (rel2par Same-Unit)
                    ( Nucleus (leaf 43) (rel2par span) (text _!Canada's declining crude output,_!) )
                    ( Satellite (leaf 44) (rel2par circumstance-e) (text _!combined with the fast-shrinking output of U.S. crude,_!) )
                  )
                  ( Nucleus (leaf 45) (rel2par Same-Unit) (text _!will help intensify U.S. reliance on oil from overseas, particularly the Middle East._!) )
                )
              )
              ( Satellite (span 46 52) (rel2par evaluation-s)
                ( Nucleus (span 46 49) (rel2par span)
                  ( Nucleus (span 46 48) (rel2par span)
                    ( Nucleus (leaf 46) (rel2par Contrast) (text _!"It's very much a growing concern._!) )
                    ( Nucleus (span 47 48) (rel2par Contrast)
                      ( Satellite (leaf 47) (rel2par condition) (text _!But when something is inevitable,_!) )
                      ( Nucleus (leaf 48) (rel2par span) (text _!you learn to live with it,"_!) )
                    )
                  )
                  ( Satellite (leaf 49) (rel2par attribution) (text _!he said.<P>_!) )
                )
                ( Satellite (span 50 52) (rel2par elaboration-additional)
                  ( Satellite (leaf 50) (rel2par attribution) (text _!Mr. Lichtblau stressed_!) )
                  ( Nucleus (span 51 52) (rel2par span)
                    ( Nucleus (leaf 51) (rel2par span) (text _!that the delay of Interprovincial's proposed expansion won't by itself increase U.S. dependence on offshore crude, however,_!) )
                    ( Satellite (leaf 52) (rel2par rhetorical-question) (text _!since Canadian imports are limited in any case by Canada's falling output.<P>_!) )
                  )
                )
              )
            )
          )
        )
      )
    )
  )
  ( Satellite (span 53 57) (rel2par circumstance)
    ( Nucleus (span 53 56) (rel2par span)
      ( Nucleus (span 53 55) (rel2par span)
        ( Nucleus (span 53 54) (rel2par Same-Unit)
          ( Nucleus (leaf 53) (rel2par span) (text _!Under terms of its proposed two-step expansion,_!) )
          ( Satellite (leaf 54) (rel2par elaboration-additional-e) (text _!which would have required regulatory approval,_!) )
        )
        ( Nucleus (leaf 55) (rel2par Same-Unit) (text _!Interprovincial intended to add 200,000 barrels a day of additional capacity to its system,_!) )
      )
      ( Satellite (leaf 56) (rel2par manner) (text _!beginning with a modest expansion by 1991._!) )
    )
    ( Satellite (leaf 57) (rel2par elaboration-additional) (text _!The system currently has a capacity of 1.55 million barrels a day._!) )
  )
)
