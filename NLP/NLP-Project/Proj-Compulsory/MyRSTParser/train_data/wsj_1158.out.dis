( Root (span 1 89)
  ( Nucleus (span 1 88) (rel2par TextualOrganization)
    ( Nucleus (span 1 49) (rel2par span)
      ( Nucleus (span 1 40) (rel2par span)
        ( Nucleus (span 1 31) (rel2par span)
          ( Nucleus (span 1 21) (rel2par span)
            ( Satellite (span 1 12) (rel2par summary-s)
              ( Nucleus (span 1 5) (rel2par span)
                ( Nucleus (leaf 1) (rel2par span) (text _!Medieval philosophers used to hold the sensible belief_!) )
                ( Satellite (span 2 5) (rel2par elaboration-object-attribute-e)
                  ( Nucleus (leaf 2) (rel2par List) (text _!that it was more perfect to exist than not to exist,_!) )
                  ( Nucleus (span 3 5) (rel2par List)
                    ( Nucleus (leaf 3) (rel2par span) (text _!and that to exist as a matter of necessity was most perfect of all._!) )
                    ( Satellite (span 4 5) (rel2par elaboration-object-attribute)
                      ( Nucleus (leaf 4) (rel2par span) (text _!Now, only God exists as a matter of absolute necessity;_!) )
                      ( Satellite (leaf 5) (rel2par restatement) (text _!it is built into His nature._!) )
                    )
                  )
                )
              )
              ( Satellite (span 6 12) (rel2par elaboration-additional)
                ( Nucleus (span 6 7) (rel2par span)
                  ( Nucleus (leaf 6) (rel2par span) (text _!But since the time of Darwin, we humans could at least claim a sort of natural necessity for the existence of our species._!) )
                  ( Satellite (leaf 7) (rel2par reason) (text _!Aren't we, after all, the inevitable culmination of that stately pageant called evolution?_!) )
                )
                ( Satellite (span 8 12) (rel2par explanation-argumentative)
                  ( Satellite (leaf 8) (rel2par condition) (text _!If mutation and natural selection slowly but surely give rise to more and more advanced forms of life,_!) )
                  ( Nucleus (span 9 12) (rel2par span)
                    ( Nucleus (leaf 9) (rel2par span) (text _!then it was only a matter of eons_!) )
                    ( Satellite (span 10 12) (rel2par temporal-before)
                      ( Nucleus (span 10 11) (rel2par Same-Unit)
                        ( Nucleus (leaf 10) (rel2par span) (text _!before splendid beings_!) )
                        ( Satellite (leaf 11) (rel2par elaboration-object-attribute-e) (text _!endowed with reason, self-awareness and taste_!) )
                      )
                      ( Nucleus (leaf 12) (rel2par Same-Unit) (text _!shimmered onto the scene.<P>_!) )
                    )
                  )
                )
              )
            )
            ( Nucleus (span 13 21) (rel2par span)
              ( Nucleus (span 13 14) (rel2par span)
                ( Nucleus (leaf 13) (rel2par span) (text _!Now along comes Stephen Jay Gould_!) )
                ( Satellite (leaf 14) (rel2par purpose) (text _!to dash this flattering illusion._!) )
              )
              ( Satellite (span 15 21) (rel2par elaboration-additional)
                ( Nucleus (leaf 15) (rel2par span) (text _!His credentials are excellent for the task._!) )
                ( Satellite (span 16 21) (rel2par elaboration-object-attribute)
                  ( Nucleus (leaf 16) (rel2par Contrast) (text _!Star lecturer at Harvard, author of numerous popular books on science, and scourge of the creationist lobby, Mr. Gould is perhaps the world's most eminent evolutionary theorist._!) )
                  ( Nucleus (span 17 21) (rel2par Contrast)
                    ( Nucleus (span 17 18) (rel2par span)
                      ( Nucleus (leaf 17) (rel2par span) (text _!Yet he puts quite a twist on the old story_!) )
                      ( Satellite (leaf 18) (rel2par elaboration-object-attribute-e) (text _!handed down from Darwin._!) )
                    )
                    ( Satellite (span 19 21) (rel2par elaboration-additional)
                      ( Nucleus (leaf 19) (rel2par span) (text _!For him, natural history is anything but a gradual, predictable march from primordial slime to human consciousness;_!) )
                      ( Satellite (span 20 21) (rel2par antithesis)
                        ( Nucleus (leaf 20) (rel2par span) (text _!it is a careening, chaotic affair_!) )
                        ( Satellite (leaf 21) (rel2par elaboration-object-attribute) (text _!in which the emergence of a featherless biped was a one-in-a-million shot.<P>_!) )
                      )
                    )
                  )
                )
              )
            )
          )
          ( Satellite (span 22 31) (rel2par elaboration-additional)
            ( Nucleus (span 22 25) (rel2par span)
              ( Nucleus (span 22 24) (rel2par Same-Unit)
                ( Nucleus (span 22 23) (rel2par span)
                  ( Nucleus (leaf 22) (rel2par span) (text _!In "Wonderful Life:_!) )
                  ( Satellite (leaf 23) (rel2par elaboration-object-attribute) (text _!The Burgess Shale and the Nature of History"_!) )
                )
                ( Satellite (leaf 24) (rel2par elaboration-object-attribute-e) (text _!(Norton, 326 pages, $19.95),_!) )
              )
              ( Nucleus (leaf 25) (rel2par Same-Unit) (text _!Mr. Gould makes his case for "the awesome improbability of human evolution."_!) )
            )
            ( Satellite (span 26 31) (rel2par background)
              ( Nucleus (span 26 27) (rel2par span)
                ( Nucleus (leaf 26) (rel2par span) (text _!The argument turns on the discovery in 1909 of an amazing fossil quarry high in the Canadian Rockies_!) )
                ( Satellite (leaf 27) (rel2par elaboration-object-attribute-e) (text _!called the Burgess Shale._!) )
              )
              ( Satellite (span 28 31) (rel2par elaboration-additional)
                ( Nucleus (span 28 29) (rel2par span)
                  ( Nucleus (leaf 28) (rel2par span) (text _!Here, in an area smaller than a city block, lay buried traces of countless weird creatures_!) )
                  ( Satellite (leaf 29) (rel2par elaboration-object-attribute-e) (text _!that had frolicked more than 500 million years ago -_!) )
                )
                ( Satellite (span 30 31) (rel2par elaboration-object-attribute)
                  ( Nucleus (leaf 30) (rel2par span) (text _!- creatures_!) )
                  ( Satellite (leaf 31) (rel2par elaboration-object-attribute-e) (text _!whose anatomical variety far exceeded what can be found in all the world's oceans today.<P>_!) )
                )
              )
            )
          )
        )
        ( Satellite (span 32 40) (rel2par elaboration-additional)
          ( Nucleus (span 32 33) (rel2par span)
            ( Nucleus (leaf 32) (rel2par span) (text _!Such an embarrassment of riches was inconceivable to the man_!) )
            ( Satellite (leaf 33) (rel2par elaboration-object-attribute-e) (text _!who discovered the Burgess Shale, one Charles Doolittle Walcott._!) )
          )
          ( Satellite (span 34 40) (rel2par elaboration-additional)
            ( Satellite (leaf 34) (rel2par attribution) (text _!The received Darwinian wisdom of the day said_!) )
            ( Nucleus (span 35 40) (rel2par span)
              ( Nucleus (span 35 38) (rel2par span)
                ( Nucleus (span 35 36) (rel2par Same-Unit)
                  ( Nucleus (leaf 35) (rel2par span) (text _!that animals_!) )
                  ( Satellite (leaf 36) (rel2par elaboration-object-attribute-e) (text _!living so long ago_!) )
                )
                ( Nucleus (span 37 38) (rel2par Same-Unit)
                  ( Nucleus (leaf 37) (rel2par List) (text _!must be simple in design,_!) )
                  ( Nucleus (leaf 38) (rel2par List) (text _!limited in scope and ancestral to contemporary species._!) )
                )
              )
              ( Satellite (span 39 40) (rel2par concession)
                ( Nucleus (leaf 39) (rel2par span) (text _!Accordingly, the hidebound traditionalist reconstructed hypothetical organisms from the Burgess fossils in such a way_!) )
                ( Satellite (leaf 40) (rel2par elaboration-object-attribute) (text _!that they could be shoehorned into familiar categories.<P>_!) )
              )
            )
          )
        )
      )
      ( Satellite (span 41 49) (rel2par antithesis)
        ( Nucleus (span 41 46) (rel2par span)
          ( Nucleus (leaf 41) (rel2par span) (text _!It was not until the early 1970s that Cambridge Prof. Harry Whittington and two sharp graduate students began to publish a reinterpretation of the Burgess Shale._!) )
          ( Satellite (span 42 46) (rel2par elaboration-additional)
            ( Nucleus (span 42 43) (rel2par Same-Unit)
              ( Nucleus (leaf 42) (rel2par span) (text _!By making clever inferences_!) )
              ( Satellite (leaf 43) (rel2par elaboration-object-attribute-e) (text _!about how the squashed and distorted fossil remains corresponded to three-dimensional structures,_!) )
            )
            ( Nucleus (span 44 46) (rel2par Same-Unit)
              ( Nucleus (leaf 44) (rel2par span) (text _!this trio was able to piece together a series of wondrous beasties quite unlike anything currently on the planet._!) )
              ( Satellite (span 45 46) (rel2par elaboration-object-attribute-e)
                ( Nucleus (leaf 45) (rel2par span) (text _!One was so fantastic in appearance,_!) )
                ( Satellite (leaf 46) (rel2par cause) (text _!it was dubbed Hallucigenia.<P>_!) )
              )
            )
          )
        )
        ( Satellite (span 47 49) (rel2par elaboration-additional)
          ( Nucleus (span 47 48) (rel2par span)
            ( Nucleus (leaf 47) (rel2par span) (text _!Would that Mr. Gould's minute descriptions of these creatures was always so colorful._!) )
            ( Satellite (leaf 48) (rel2par antithesis) (text _!A good deal of the book is boring, particularly the endless allusions to high and pop culture and the frequent jokes festooning the text._!) )
          )
          ( Satellite (leaf 49) (rel2par example) (text _!These turns do not provide sufficient relief from sentences like, "Most modern chelicerates have six uniramous appendages on the prosoma."<P>_!) )
        )
      )
    )
    ( Satellite (span 50 88) (rel2par elaboration-general-specific)
      ( Nucleus (span 50 51) (rel2par span)
        ( Nucleus (leaf 50) (rel2par span) (text _!Interest picks up, though,_!) )
        ( Satellite (leaf 51) (rel2par temporal-same-time) (text _!when Mr. Gould gets around to discussing the meaning of the Burgess oddities for the theory of evolution._!) )
      )
      ( Satellite (span 52 88) (rel2par elaboration-additional)
        ( Nucleus (span 52 53) (rel2par span)
          ( Nucleus (leaf 52) (rel2par span) (text _!Not long after the appearance of life, evidently, there was an explosive proliferation in the number of animal designs_!) )
          ( Satellite (leaf 53) (rel2par elaboration-object-attribute-e) (text _!seen on the earth._!) )
        )
        ( Satellite (span 54 88) (rel2par concession)
          ( Nucleus (span 54 56) (rel2par span)
            ( Nucleus (leaf 54) (rel2par span) (text _!The vast majority of them, however, were wiped out by a succession of environmental upheavals_!) )
            ( Satellite (span 55 56) (rel2par elaboration-object-attribute-e)
              ( Nucleus (leaf 55) (rel2par span) (text _!that were too sudden and catastrophic_!) )
              ( Satellite (leaf 56) (rel2par circumstance) (text _!for the normal rules of natural selection to operate._!) )
            )
          )
          ( Satellite (span 57 88) (rel2par comment)
            ( Nucleus (span 57 59) (rel2par span)
              ( Nucleus (leaf 57) (rel2par span) (text _!Consequently, the winnowing process was like a lottery_!) )
              ( Satellite (span 58 59) (rel2par elaboration-object-attribute)
                ( Nucleus (leaf 58) (rel2par span) (text _!"in which each group {held} a ticket_!) )
                ( Satellite (leaf 59) (rel2par elaboration-object-attribute-e) (text _!unrelated to its anatomical virtues."_!) )
              )
            )
            ( Satellite (span 60 88) (rel2par comment)
              ( Nucleus (leaf 60) (rel2par List) (text _!So much for survival of the fittest.<P>_!) )
              ( Nucleus (span 61 88) (rel2par List)
                ( Nucleus (span 61 63) (rel2par span)
                  ( Nucleus (leaf 61) (rel2par span) (text _!So much, too, for the notion_!) )
                  ( Satellite (span 62 63) (rel2par elaboration-object-attribute-e)
                    ( Nucleus (leaf 62) (rel2par span) (text _!that we humans triumphed in the Darwinian struggle_!) )
                    ( Satellite (leaf 63) (rel2par cause) (text _!by evolving big brains._!) )
                  )
                )
                ( Satellite (span 64 88) (rel2par antithesis)
                  ( Nucleus (span 64 66) (rel2par span)
                    ( Nucleus (span 64 65) (rel2par span)
                      ( Nucleus (leaf 64) (rel2par span) (text _!Our mammalian forerunners lucked out through the extraterrestrial impact_!) )
                      ( Satellite (leaf 65) (rel2par elaboration-object-attribute-e) (text _!that did in the dinosaurs_!) )
                    )
                    ( Satellite (leaf 66) (rel2par cause) (text _!because they were small, not smart._!) )
                  )
                  ( Satellite (span 67 88) (rel2par hypothetical)
                    ( Satellite (span 67 68) (rel2par condition)
                      ( Nucleus (leaf 67) (rel2par span) (text _!If anyone has difficulty imagining a world_!) )
                      ( Satellite (leaf 68) (rel2par elaboration-object-attribute-e) (text _!in which history went merrily on without us,_!) )
                    )
                    ( Nucleus (span 69 88) (rel2par span)
                      ( Nucleus (leaf 69) (rel2par span) (text _!Mr. Gould sketches several._!) )
                      ( Satellite (span 70 88) (rel2par example)
                        ( Nucleus (leaf 70) (rel2par List) (text _!In one, birds are the dominant carnivores;_!) )
                        ( Nucleus (span 71 88) (rel2par List)
                          ( Nucleus (leaf 71) (rel2par span) (text _!in another the seas abound with "little penises."<P>_!) )
                          ( Satellite (span 72 88) (rel2par elaboration-object-attribute)
                            ( Nucleus (span 72 75) (rel2par span)
                              ( Satellite (leaf 72) (rel2par temporal-same-time) (text _!Back when the Burgess fauna were flourishing,_!) )
                              ( Nucleus (span 73 75) (rel2par span)
                                ( Satellite (leaf 73) (rel2par comment) (text _!it seems,_!) )
                                ( Nucleus (span 74 75) (rel2par span)
                                  ( Nucleus (leaf 74) (rel2par span) (text _!human evolutionary hopes hung on the survival of a little worm with a backbone_!) )
                                  ( Satellite (leaf 75) (rel2par elaboration-object-attribute-e) (text _!called Pikaia._!) )
                                )
                              )
                            )
                            ( Satellite (span 76 88) (rel2par comment)
                              ( Nucleus (span 76 80) (rel2par span)
                                ( Nucleus (span 76 77) (rel2par Contrast)
                                  ( Nucleus (leaf 76) (rel2par span) (text _!Mr. Gould finds this oddly exhilarating;_!) )
                                  ( Satellite (leaf 77) (rel2par elaboration-additional) (text _!like an existentialist of old, he views our contingency as "a source of both freedom and consequent moral responsibility."_!) )
                                )
                                ( Nucleus (span 78 80) (rel2par Contrast)
                                  ( Nucleus (leaf 78) (rel2par span) (text _!I, by contrast, cannot help feeling_!) )
                                  ( Satellite (span 79 80) (rel2par elaboration-additional)
                                    ( Satellite (leaf 79) (rel2par condition) (text _!that if some other curiosity from the Burgess Shale had survived instead,_!) )
                                    ( Nucleus (leaf 80) (rel2par span) (text _!beings at once wiser and less boorish than Homo sapiens might have eventually gained earthly dominion._!) )
                                  )
                                )
                              )
                              ( Satellite (span 81 88) (rel2par interpretation-s)
                                ( Nucleus (span 81 84) (rel2par span)
                                  ( Satellite (leaf 81) (rel2par hypothetical) (text _!But even if no conscious life had evolved here at all,_!) )
                                  ( Nucleus (span 82 84) (rel2par span)
                                    ( Nucleus (leaf 82) (rel2par List) (text _!the universe is a big place,_!) )
                                    ( Nucleus (span 83 84) (rel2par List)
                                      ( Satellite (leaf 83) (rel2par condition) (text _!and given the right conditions,_!) )
                                      ( Nucleus (leaf 84) (rel2par span) (text _!sympathetic to creating some form of life._!) )
                                    )
                                  )
                                )
                                ( Satellite (span 85 88) (rel2par consequence-n)
                                  ( Nucleus (leaf 85) (rel2par span) (text _!Surely at some other cosmic address a Gouldoid creature would have risen out of the ooze_!) )
                                  ( Satellite (span 86 88) (rel2par purpose)
                                    ( Nucleus (span 86 87) (rel2par span)
                                      ( Nucleus (leaf 86) (rel2par span) (text _!to explain why,_!) )
                                      ( Satellite (leaf 87) (rel2par manner-e) (text _!paleontologically speaking,_!) )
                                    )
                                    ( Satellite (leaf 88) (rel2par reason) (text _!"it is, indeed, a wonderful life."<P>_!) )
                                  )
                                )
                              )
                            )
                          )
                        )
                      )
                    )
                  )
                )
              )
            )
          )
        )
